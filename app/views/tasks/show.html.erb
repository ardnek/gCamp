<div class="container">
<p id="notice"><%= notice %></p>

<p>
  <strong>Description:</strong>
  <%= @task.description %>
</p>

<p>
  <strong>Completed:</strong>
  <%= @task.complete %>
</p>

<p>
  <strong>Due date:</strong>
  <%= @task.date.try(:strftime, "%m/%d/%Y") %>
</p>

<%= link_to 'Edit', edit_project_task_path(@project, @task) %> |
<%= link_to 'Back', project_tasks_path %>
</div>
