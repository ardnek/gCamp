<div class="container">
<h3>Projects</h3>

<div class="pull-right">
<%= link_to "Create Project", new_project_path, :class => "btn btn-info btn-sm" %>
</div>


<table class="table table-condensed">
  <thead>
    <tr>
    <th>Name</th>
    </tr>

  </thead>

  <tbody>
    <% @projects.each do |project| %>
    <tr>
      <td><%= link_to project.name, project_path(project) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>
